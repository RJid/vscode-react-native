                {
                    "type": "reactnative",
                    "label": "React Native",
                    "runtime": "node",
                    "configurationSnippets": [],
                    "configurationAttributes": {
                        "attach": {
                            "required": ["cwd"],
                            "properties": {
                                "program": {
                                    "type": "string",
                                    "description": "%reactNative.attach.program.description%"
                                },
                                "env": {
                                    "type": "object",
                                    "additionalProperties": {
                                        "type": "string"
                                    },
                                    "description": "%reactNative.attach.env.description%",
                                    "default": {}
                                },
                                "envFile": {
